    "./lib/*.js": "./lib/src/*.js",
    "./lib/*": "./lib/src/*.js",
    "./*": "./*"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.1",
    "gulp": "^4.0.2"
