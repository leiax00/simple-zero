{
  "name": "@leiax00/build",
  "description": "",
  "scripts": {
    "start": "gulp --require @esbuild-kit/cjs-loader -f gulpfile.ts",
    "build": "vite build"
  },
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "unpkg": "src/index.ts",
  "exports": {
    ".": {
